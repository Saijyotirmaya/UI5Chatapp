<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
        id="TSF_id_Dialog"
        title="{i18n>selectAssignment}"
        items="{AssignModel>/}"
        confirm=".onValueHelpDialogClose"
        cancel=".onValueHelpCancel"
        liveChange=".onAssignmentLiveChange">
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{AssignModel>TaskName}"
            description="{AssignModel>TaskID}" />
    </SelectDialog>
</core:FragmentDefinition>